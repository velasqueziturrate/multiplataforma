{
  "name": "nativescript-worker-loader",
  "version": "0.12.1",
  "author": "NativeScript team",
  "description": "nativescript worker loader module for webpack",
  "scripts": {
    "pretest": "babel ./test/test.es6.js --out-file ./test/test.es5.js",
    "test": "mocha test/test.es5.js --timeout 5000",
    "posttest": "eslint .",
    "prepare": "npm run test"
  },
  "eslintConfig": {
    "extends": "webpack",
    "rules": {
      "quotes": [
        "error",
        "double"
      ],
      "indent": [
        "error",
        4
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "never"
        }
      ],
      "no-console": 0,
      "arrow-parens": 0,
      "func-names": 0,
      "import/no-extraneous-dependencies": 0,
      "linebreak-style": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "prefer-destructuring": 0,
      "strict": 0,
      "consistent-return": 0
    }
  },
  "dependencies": {
    "loader-utils": "^2.0.0",
    "schema-utils": "^2.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "del": "^5.1.0",
    "eslint": "^7.6.0",
    "eslint-config-webpack": "^1.0.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^8.1.1",
    "webpack": "~4.44.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NativeScript/worker-loader.git"
  },
  "license": "MIT"

,"_resolved": "https://registry.npmjs.org/nativescript-worker-loader/-/nativescript-worker-loader-0.12.1.tgz"
,"_integrity": "sha512-phzXAmeqcPBHTlRTFKkbjVjpnisdXQM2VqIl4y/gfMHKG2y78bJNLQPTzsPSENK9V075FIS8nSs1WcrDav8yag=="
,"_from": "nativescript-worker-loader@0.12.1"
}